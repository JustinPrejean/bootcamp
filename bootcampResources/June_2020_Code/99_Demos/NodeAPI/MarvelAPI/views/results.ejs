<%- include('partials/header.ejs')  %> 

<h1>Results:</h1>

<% data.forEach(el => { %>  
    <form action='/add'>
        <h3><%= el.name %></h3>
        <% let url = `${el.thumbnail.path}/portrait_uncanny.${el.thumbnail.extension}` %> 
        <img src=<%= url %> alt=<%= el.name %>>
        <input type='text' name="charId" value="<%= el.id %>">
        <button type="submit">Add to DB</button>
    </form>
<% }) %> 
